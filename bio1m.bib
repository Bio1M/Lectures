@article{williams_sickle_2005,
	title = {Sickle Cell Trait and the Risk of Plasmodium falciparum Malaria and Other Childhood Diseases},
	volume = {192},
	issn = {0022-1899, 1537-6613},
	url = {http://jid.oxfordjournals.org/content/192/1/178},
	doi = {10.1086/430744},
	abstract = {{BackgroundThe} gene for sickle hemoglobin ({HbS}) is a prime example of natural selection. It is generally believed that its current prevalence in many tropical populations reflects selection for the carrier form (sickle cell trait [{HbAS}]) through a survival advantage against death from malaria. Nevertheless, {\textgreater}50 years after this hypothesis was first proposed, the epidemiological description of the relationships between {HbAS}, malaria, and other common causes of child mortality remains incomplete
{MethodsWe} studied the incidence of falciparum malaria and other childhood diseases in 2 cohorts of children living on the coast of Kenya
{ResultsThe} protective effect of {HbAS} was remarkably specific for falciparum malaria, having no significant impact on any other disease. {HbAS} had no effect on the prevalence of symptomless parasitemia but was 50\% protective against mild clinical malaria, 75\% protective against admission to the hospital for malaria, and almost 90\% protective against severe or complicated malaria. The effect of {HbAS} on episodes of clinical malaria was mirrored in its effect on parasite densities during such episodes
{ConclusionsThe} present data are useful in that they confirm the mechanisms by which {HbAS} confers protection against malaria and shed light on the relationships between {HbAS}, malaria, and other childhood diseases},
	language = {en},
	number = {1},
	urldate = {2014-10-18},
	journal = {Journal of Infectious Diseases},
	author = {Williams, Thomas N. and Mwangi, Tabitha W. and Wambua, Sammy and Alexander, Neal D. and Kortok, Moses and Snow, Robert W. and Marsh, Kevin},
	month = jul,
	year = {2005},
	pmid = {15942909},
	pages = {178--186},
	file = {Snapshot:/home/bolker/.mozilla/firefox/gf2kp1m5.default/zotero/storage/CNQK8S3Z/178.html:text/html;Williams et al_2005_Sickle Cell Trait and the Risk of Plasmodium falciparum Malaria and Other.pdf:/home/bolker/.mozilla/firefox/gf2kp1m5.default/zotero/storage/3QR2JBSJ/Williams et al_2005_Sickle Cell Trait and the Risk of Plasmodium falciparum Malaria and Other.pdf:application/pdf}
